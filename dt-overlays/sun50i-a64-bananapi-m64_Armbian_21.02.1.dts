/dts-v1/;

/ {
	interrupt-parent = <0x1>;
	#address-cells = <0x1>;
	#size-cells = <0x1>;
	model = "BananaPi-M64";
	compatible = "sinovoip,bananapi-m64", "allwinner,sun50i-a64";

	aliases {
		mmc0 = "/soc/mmc@1c0f000";
		mmc1 = "/soc/mmc@1c10000";
		mmc2 = "/soc/mmc@1c11000";
		ethernet0 = "/soc/ethernet@1c30000";
		serial0 = "/soc/serial@1c28000";
		serial1 = "/soc/serial@1c28400";
	};

	chosen {
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		ranges;
		stdout-path = "serial0:115200n8";

		framebuffer-lcd {
			compatible = "allwinner,simple-framebuffer", "simple-framebuffer";
			allwinner,pipeline = "mixer0-lcd0";
			clocks = <0x2 0x64 0x3 0x6>;
			status = "disabled";
			phandle = <0x53>;
		};

		framebuffer-hdmi {
			compatible = "allwinner,simple-framebuffer", "simple-framebuffer";
			allwinner,pipeline = "mixer1-lcd1-hdmi";
			clocks = <0x3 0x7 0x2 0x65 0x2 0x6e>;
			status = "disabled";
			vcc-hdmi-supply = <0x4>;
			phandle = <0x54>;
		};
	};

	cpus {
		#address-cells = <0x1>;
		#size-cells = <0x0>;

		cpu@0 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x0>;
			enable-method = "psci";
			next-level-cache = <0x5>;
			clocks = <0x2 0x15>;
			clock-names = "cpu";
			#cooling-cells = <0x2>;
			operating-points-v2 = <0x6>;
			cpu-supply = <0x7>;
			phandle = <0xc>;
		};

		cpu@1 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x1>;
			enable-method = "psci";
			next-level-cache = <0x5>;
			clocks = <0x2 0x15>;
			clock-names = "cpu";
			#cooling-cells = <0x2>;
			operating-points-v2 = <0x6>;
			cpu-supply = <0x7>;
			phandle = <0xd>;
		};

		cpu@2 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x2>;
			enable-method = "psci";
			next-level-cache = <0x5>;
			clocks = <0x2 0x15>;
			clock-names = "cpu";
			#cooling-cells = <0x2>;
			operating-points-v2 = <0x6>;
			cpu-supply = <0x7>;
			phandle = <0xe>;
		};

		cpu@3 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x3>;
			enable-method = "psci";
			next-level-cache = <0x5>;
			clocks = <0x2 0x15>;
			clock-names = "cpu";
			#cooling-cells = <0x2>;
			operating-points-v2 = <0x6>;
			cpu-supply = <0x7>;
			phandle = <0xf>;
		};

		l2-cache {
			compatible = "cache";
			cache-level = <0x2>;
			phandle = <0x5>;
		};
	};

	display-engine {
		compatible = "allwinner,sun50i-a64-display-engine";
		allwinner,pipelines = <0x8 0x9>;
		status = "okay";
		phandle = <0x55>;
	};

	hdmi-sound {
		compatible = "simple-audio-card";
		simple-audio-card,format = "i2s";
		simple-audio-card,name = "sun50i-a64-hdmi";
		simple-audio-card,mclk-fs = <0x80>;
		simple-audio-card,frame-inversion;
		status = "disabled";
		phandle = <0x56>;

		simple-audio-card,codec {
			sound-dai = <0xa>;
		};

		simple-audio-card,cpu {
			sound-dai = <0xb>;
			dai-tdm-slot-num = <0x2>;
			dai-tdm-slot-width = <0x20>;
		};
	};

	osc24M_clk {
		#clock-cells = <0x0>;
		compatible = "fixed-clock";
		clock-frequency = <0x16e3600>;
		clock-output-names = "osc24M";
		phandle = <0x32>;
	};

	osc32k_clk {
		#clock-cells = <0x0>;
		compatible = "fixed-clock";
		clock-frequency = <0x8000>;
		clock-output-names = "ext-osc32k";
		phandle = <0x4c>;
	};

	pmu {
		compatible = "arm,cortex-a53-pmu";
		interrupts = <0x0 0x74 0x4 0x0 0x75 0x4 0x0 0x76 0x4 0x0 0x77 0x4>;
		interrupt-affinity = <0xc 0xd 0xe 0xf>;
	};

	psci {
		compatible = "arm,psci-0.2";
		method = "smc";
	};

	scpi {
		compatible = "arm,scpi";
		mboxes = <0x10 0x2 0x10 0x3>;
		mbox-names = "tx", "rx";
		shmem = <0x11>;
		phandle = <0x57>;
	};

	sound {
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		compatible = "simple-audio-card";
		simple-audio-card,name = "sun50i-a64-audio";
		simple-audio-card,aux-devs = <0x12>;
		simple-audio-card,routing = "Left DAC", "DACL", "Right DAC", "DACR", "ADCL", "Left ADC", "ADCR", "Right ADC", "Headphone Jack", "HP", "MIC2", "Microphone Jack", "Onboard Microphone", "MBIAS", "MIC1", "Onboard Microphone";
		status = "okay";
		simple-audio-card,widgets = "Headphone", "Headphone Jack", "Microphone", "Microphone Jack", "Microphone", "Onboard Microphone";
		phandle = <0x58>;

		simple-audio-card,dai-link@0 {
			format = "i2s";
			frame-master = <0x13>;
			bitclock-master = <0x13>;
			mclk-fs = <0x80>;

			cpu {
				sound-dai = <0x14>;
				phandle = <0x13>;
			};

			codec {
				sound-dai = <0x15 0x0>;
				phandle = <0x59>;
			};
		};
	};

	timer {
		compatible = "arm,armv8-timer";
		allwinner,erratum-unknown1;
		arm,no-tick-in-suspend;
		interrupts = <0x1 0xd 0xf04 0x1 0xe 0xf04 0x1 0xb 0xf04 0x1 0xa 0xf04>;
	};

	thermal-zones {

		cpu0-thermal {
			polling-delay-passive = <0x0>;
			polling-delay = <0x0>;
			thermal-sensors = <0x16 0x0>;
			phandle = <0x5a>;

			cooling-maps {

				map0 {
					trip = <0x17>;
					cooling-device = <0xc 0xffffffff 0xffffffff 0xd 0xffffffff 0xffffffff 0xe 0xffffffff 0xffffffff 0xf 0xffffffff 0xffffffff>;
				};

				map1 {
					trip = <0x18>;
					cooling-device = <0xc 0xffffffff 0xffffffff 0xd 0xffffffff 0xffffffff 0xe 0xffffffff 0xffffffff 0xf 0xffffffff 0xffffffff>;
				};
			};

			trips {

				cpu_alert0 {
					temperature = <0x11170>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x17>;
				};

				cpu_alert1 {
					temperature = <0x13880>;
					hysteresis = <0x7d0>;
					type = "hot";
					phandle = <0x18>;
				};

				cpu_crit {
					temperature = <0x15f90>;
					hysteresis = <0x7d0>;
					type = "critical";
					phandle = <0x5b>;
				};
			};
		};

		gpu0-thermal {
			polling-delay-passive = <0x0>;
			polling-delay = <0x0>;
			thermal-sensors = <0x16 0x1>;
			phandle = <0x5c>;
		};

		gpu1-thermal {
			polling-delay-passive = <0x0>;
			polling-delay = <0x0>;
			thermal-sensors = <0x16 0x2>;
			phandle = <0x5d>;
		};
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		ranges;

		bus@1000000 {
			compatible = "allwinner,sun50i-a64-de2";
			reg = <0x1000000 0x400000>;
			allwinner,sram = <0x19 0x1>;
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			ranges = <0x0 0x1000000 0x400000>;

			clock@0 {
				compatible = "allwinner,sun50i-a64-de2-clk";
				reg = <0x0 0x10000>;
				clocks = <0x2 0x34 0x2 0x63>;
				clock-names = "bus", "mod";
				resets = <0x2 0x1e>;
				#clock-cells = <0x1>;
				#reset-cells = <0x1>;
				phandle = <0x3>;
			};

			rotate@20000 {
				compatible = "allwinner,sun50i-a64-de2-rotate", "allwinner,sun8i-a83t-de2-rotate";
				reg = <0x20000 0x10000>;
				interrupts = <0x0 0x60 0x4>;
				clocks = <0x3 0x9 0x3 0xa>;
				clock-names = "bus", "mod";
				resets = <0x3 0x3>;
				phandle = <0x5e>;
			};

			mixer@100000 {
				compatible = "allwinner,sun50i-a64-de2-mixer-0";
				reg = <0x100000 0x100000>;
				clocks = <0x3 0x0 0x3 0x6>;
				clock-names = "bus", "mod";
				resets = <0x3 0x0>;
				phandle = <0x8>;

				ports {
					#address-cells = <0x1>;
					#size-cells = <0x0>;

					port@1 {
						#address-cells = <0x1>;
						#size-cells = <0x0>;
						reg = <0x1>;
						phandle = <0x5f>;

						endpoint@0 {
							reg = <0x0>;
							remote-endpoint = <0x1a>;
							phandle = <0x1e>;
						};

						endpoint@1 {
							reg = <0x1>;
							remote-endpoint = <0x1b>;
							phandle = <0x21>;
						};
					};
				};
			};

			mixer@200000 {
				compatible = "allwinner,sun50i-a64-de2-mixer-1";
				reg = <0x200000 0x100000>;
				clocks = <0x3 0x1 0x3 0x7>;
				clock-names = "bus", "mod";
				resets = <0x3 0x1>;
				phandle = <0x9>;

				ports {
					#address-cells = <0x1>;
					#size-cells = <0x0>;

					port@1 {
						#address-cells = <0x1>;
						#size-cells = <0x0>;
						reg = <0x1>;
						phandle = <0x60>;

						endpoint@0 {
							reg = <0x0>;
							remote-endpoint = <0x1c>;
							phandle = <0x1f>;
						};

						endpoint@1 {
							reg = <0x1>;
							remote-endpoint = <0x1d>;
							phandle = <0x22>;
						};
					};
				};
			};
		};

		syscon@1c00000 {
			compatible = "allwinner,sun50i-a64-system-control";
			reg = <0x1c00000 0x1000>;
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			ranges;
			phandle = <0x3f>;

			sram@40000 {
				compatible = "mmio-sram";
				reg = <0x40000 0x14000>;
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				ranges = <0x0 0x40000 0x14000>;
				phandle = <0x61>;

				scp-shmem@13c00 {
					compatible = "arm,scp-shmem";
					reg = <0x13c00 0x200>;
					phandle = <0x11>;
				};
			};

			sram@18000 {
				compatible = "mmio-sram";
				reg = <0x18000 0x28000>;
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				ranges = <0x0 0x18000 0x28000>;
				phandle = <0x62>;

				sram-section@0 {
					compatible = "allwinner,sun50i-a64-sram-c";
					reg = <0x0 0x28000>;
					phandle = <0x19>;
				};
			};

			sram@1d00000 {
				compatible = "mmio-sram";
				reg = <0x1d00000 0x40000>;
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				ranges = <0x0 0x1d00000 0x40000>;
				phandle = <0x63>;

				sram-section@0 {
					compatible = "allwinner,sun50i-a64-sram-c1", "allwinner,sun4i-a10-sram-c1";
					reg = <0x0 0x40000>;
					phandle = <0x24>;
				};
			};
		};

		dma-controller@1c02000 {
			compatible = "allwinner,sun50i-a64-dma";
			reg = <0x1c02000 0x1000>;
			interrupts = <0x0 0x32 0x4>;
			clocks = <0x2 0x1e>;
			dma-channels = <0x8>;
			dma-requests = <0x1b>;
			resets = <0x2 0x7>;
			#dma-cells = <0x1>;
			phandle = <0x34>;
		};

		lcd-controller@1c0c000 {
			compatible = "allwinner,sun50i-a64-tcon-lcd", "allwinner,sun8i-a83t-tcon-lcd";
			reg = <0x1c0c000 0x1000>;
			interrupts = <0x0 0x56 0x4>;
			clocks = <0x2 0x2f 0x2 0x64>;
			clock-names = "ahb", "tcon-ch0";
			clock-output-names = "tcon-pixel-clock";
			#clock-cells = <0x0>;
			resets = <0x2 0x18 0x2 0x23>;
			reset-names = "lcd", "lvds";
			phandle = <0x64>;

			ports {
				#address-cells = <0x1>;
				#size-cells = <0x0>;

				port@0 {
					#address-cells = <0x1>;
					#size-cells = <0x0>;
					reg = <0x0>;
					phandle = <0x65>;

					endpoint@0 {
						reg = <0x0>;
						remote-endpoint = <0x1e>;
						phandle = <0x1a>;
					};

					endpoint@1 {
						reg = <0x1>;
						remote-endpoint = <0x1f>;
						phandle = <0x1c>;
					};
				};

				port@1 {
					#address-cells = <0x1>;
					#size-cells = <0x0>;
					reg = <0x1>;
					phandle = <0x66>;

					endpoint@1 {
						reg = <0x1>;
						remote-endpoint = <0x20>;
						allwinner,tcon-channel = <0x1>;
						phandle = <0x46>;
					};
				};
			};
		};

		lcd-controller@1c0d000 {
			compatible = "allwinner,sun50i-a64-tcon-tv", "allwinner,sun8i-a83t-tcon-tv";
			reg = <0x1c0d000 0x1000>;
			interrupts = <0x0 0x57 0x4>;
			clocks = <0x2 0x30 0x2 0x65>;
			clock-names = "ahb", "tcon-ch1";
			resets = <0x2 0x19>;
			reset-names = "lcd";
			phandle = <0x67>;

			ports {
				#address-cells = <0x1>;
				#size-cells = <0x0>;

				port@0 {
					#address-cells = <0x1>;
					#size-cells = <0x0>;
					reg = <0x0>;
					phandle = <0x68>;

					endpoint@0 {
						reg = <0x0>;
						remote-endpoint = <0x21>;
						phandle = <0x1b>;
					};

					endpoint@1 {
						reg = <0x1>;
						remote-endpoint = <0x22>;
						phandle = <0x1d>;
					};
				};

				port@1 {
					#address-cells = <0x1>;
					#size-cells = <0x0>;
					reg = <0x1>;
					phandle = <0x69>;

					endpoint@1 {
						reg = <0x1>;
						remote-endpoint = <0x23>;
						phandle = <0x49>;
					};
				};
			};
		};

		video-codec@1c0e000 {
			compatible = "allwinner,sun50i-a64-video-engine";
			reg = <0x1c0e000 0x1000>;
			clocks = <0x2 0x2e 0x2 0x6a 0x2 0x5f>;
			clock-names = "ahb", "mod", "ram";
			resets = <0x2 0x17>;
			interrupts = <0x0 0x3a 0x4>;
			allwinner,sram = <0x24 0x1>;
		};

		mmc@1c0f000 {
			compatible = "allwinner,sun50i-a64-mmc";
			reg = <0x1c0f000 0x1000>;
			clocks = <0x2 0x1f 0x2 0x4b>;
			clock-names = "ahb", "mmc";
			resets = <0x2 0x8>;
			reset-names = "ahb";
			interrupts = <0x0 0x3c 0x4>;
			max-frequency = <0x8f0d180>;
			status = "okay";
			bus-width = <0x4>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			pinctrl-names = "default";
			pinctrl-0 = <0x25>;
			vmmc-supply = <0x26>;
			cd-gpios = <0x27 0x5 0x6 0x1>;
			disable-wp;
			phandle = <0x6a>;
		};

		mmc@1c10000 {
			compatible = "allwinner,sun50i-a64-mmc";
			reg = <0x1c10000 0x1000>;
			clocks = <0x2 0x20 0x2 0x4c>;
			clock-names = "ahb", "mmc";
			resets = <0x2 0x9>;
			reset-names = "ahb";
			interrupts = <0x0 0x3d 0x4>;
			max-frequency = <0x8f0d180>;
			status = "okay";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			pinctrl-names = "default";
			pinctrl-0 = <0x28>;
			vmmc-supply = <0x29>;
			vqmmc-supply = <0x2a>;
			mmc-pwrseq = <0x2b>;
			bus-width = <0x4>;
			non-removable;
			phandle = <0x6b>;

			wifi@1 {
				reg = <0x1>;
				compatible = "brcm,bcm4329-fmac";
				interrupt-parent = <0x2c>;
				interrupts = <0x0 0x3 0x8>;
				interrupt-names = "host-wake";
				phandle = <0x6c>;
			};
		};

		mmc@1c11000 {
			compatible = "allwinner,sun50i-a64-emmc";
			reg = <0x1c11000 0x1000>;
			clocks = <0x2 0x21 0x2 0x4d>;
			clock-names = "ahb", "mmc";
			resets = <0x2 0xa>;
			reset-names = "ahb";
			interrupts = <0x0 0x3e 0x4>;
			max-frequency = <0xbebc200>;
			status = "okay";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			pinctrl-names = "default";
			pinctrl-0 = <0x2d 0x2e>;
			vmmc-supply = <0x26>;
			bus-width = <0x8>;
			non-removable;
			cap-mmc-hw-reset;
			phandle = <0x6d>;
		};

		eeprom@1c14000 {
			compatible = "allwinner,sun50i-a64-sid";
			reg = <0x1c14000 0x400>;
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			phandle = <0x6e>;

			thermal-sensor-calibration@34 {
				reg = <0x34 0x8>;
				phandle = <0x36>;
			};
		};

		crypto@1c15000 {
			compatible = "allwinner,sun50i-a64-crypto";
			reg = <0x1c15000 0x1000>;
			interrupts = <0x0 0x5e 0x4>;
			clocks = <0x2 0x1d 0x2 0x4f>;
			clock-names = "bus", "mod";
			resets = <0x2 0x6>;
			phandle = <0x6f>;
		};

		mailbox@1c17000 {
			compatible = "allwinner,sun50i-a64-msgbox", "allwinner,sun6i-a31-msgbox";
			reg = <0x1c17000 0x1000>;
			clocks = <0x2 0x36>;
			resets = <0x2 0x20>;
			interrupts = <0x0 0x31 0x4>;
			#mbox-cells = <0x1>;
			phandle = <0x10>;
		};

		usb@1c19000 {
			compatible = "allwinner,sun8i-a33-musb";
			reg = <0x1c19000 0x400>;
			clocks = <0x2 0x29>;
			resets = <0x2 0x12>;
			interrupts = <0x0 0x47 0x4>;
			interrupt-names = "mc";
			phys = <0x2f 0x0>;
			phy-names = "usb";
			extcon = <0x2f 0x0>;
			dr_mode = "otg";
			status = "okay";
			phandle = <0x70>;
		};

		phy@1c19400 {
			compatible = "allwinner,sun50i-a64-usb-phy";
			reg = <0x1c19400 0x14 0x1c1a800 0x4 0x1c1b800 0x4>;
			reg-names = "phy_ctrl", "pmu0", "pmu1";
			clocks = <0x2 0x56 0x2 0x57>;
			clock-names = "usb0_phy", "usb1_phy";
			resets = <0x2 0x0 0x2 0x1>;
			reset-names = "usb0_reset", "usb1_reset";
			status = "okay";
			#phy-cells = <0x1>;
			usb0_id_det-gpios = <0x27 0x7 0x9 0x0>;
			usb0_vbus_power-supply = <0x30>;
			usb0_vbus-supply = <0x31>;
			phandle = <0x2f>;
		};

		usb@1c1a000 {
			compatible = "allwinner,sun50i-a64-ehci", "generic-ehci";
			reg = <0x1c1a000 0x100>;
			interrupts = <0x0 0x48 0x4>;
			clocks = <0x2 0x2c 0x2 0x2a 0x2 0x5b>;
			resets = <0x2 0x15 0x2 0x13>;
			status = "okay";
			phandle = <0x71>;
		};

		usb@1c1a400 {
